{
    "name": "node-js-getting-started",
    "version": "0.3.0",
    "description": "Exemplo de Deploy de API Node no Heroku",
    "engines": {
        "node": "14.x"
    },
    "main": "index.js",
    "scripts": {
        "knexconfig": "npx knex migrate:latest && npx knex seed:run",
        "sync": "git push heroku master",
        "start": "npx knex migrate:latest && npx knex seed:run && node src/index.js",
        "dev": "npx knex migrate:latest && npx knex seed:run && nodemon --inspect src/index.js",
        "start-gendoc": "node swagger.js"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv-safe": "^8.2.0",
        "ejs": "^3.1.5",
        "eslint": "^7.32.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "jwt-decode": "^3.1.2",
        "knex": "^0.95.9",
        "md5": "^2.3.0",
        "pg": "^8.7.1",
        "sqlite3": "^5.0.2",
        "swagger-autogen": "^2.11.1",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "^4.1.6"
    },
    "devDependencies": {
        "got": "^11.3.0",
        "nodemon": "^2.0.12",
        "tape": "^4.7.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/claudneysessa/exemplo-heroku"
    },
    "keywords": [
        "node",
        "heroku",
        "express"
    ],
    "license": "MIT"
}
